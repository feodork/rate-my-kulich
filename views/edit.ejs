<%- include ("./partials/head.ejs") %>
<div>
  <h1>Update <%= theKulich.name %></h1>
  <form action="<%= baseUrl %>/<%= theKulich.id %>?_method=PUT" method="post">
    <label for="name">Name</label>
    <input type="text" name="name" id="name" required value="<%= theKulich.name %>" />

    <label for="image">Image URL</label>
    <input type="text" name="image" id="image">

    <input type="submit" value="Update Kulich" />
  </form>
</div>
<%- include ("./partials/footer.ejs") %>